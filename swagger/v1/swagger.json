    "/api/v1/rates": {
      "get": {
        "summary": "list rates",
        "tags": [
          "Rates"
        ],
        "parameters": [
          {
            "name": "origin",
            "in": "query",
            "example": 566,
            "description": "origin sub-district id",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "destination",
            "in": "query",
            "example": 456,
            "description": "destination sub-district id",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "weight",
            "in": "query",
            "example": 1000,
            "description": "weight in grams",
            "required": true,
            "schema": {
              "type": "integer"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "successful",
            "content": {
              "application/json": {
                "example": {
                  "success": true,
                  "message": "Rates fetched successfully",
                  "courier": "JNE",
                  "rates": [
                    {
                      "name": "Jalur Nugraha Ekakurir (JNE)",
                      "code": "jne",
                      "service": "CTC",
                      "description": "JNE City Courier",
                      "cost": 8000,
                      "etd": "1 day"
                    },
                    {
                      "name": "Jalur Nugraha Ekakurir (JNE)",
                      "code": "jne",
                      "service": "CTCYES",
                      "description": "JNE City Courier",
                      "cost": 12000,
                      "etd": "1 day"
                    },
                    {
                      "name": "Jalur Nugraha Ekakurir (JNE)",
                      "code": "jne",
                      "service": "CTCSPS",
