                  "type": "object",
                  "properties": {
                    "success": {
                      "type": "boolean",
                      "example": false
                    },
                    "error": {
                      "type": "object",
                      "properties": {
                        "code": {
                          "type": "string",
                          "example": "BAD_REQUEST"
                        },
                        "message": {
                          "type": "string",
                          "example": "Invalid request or courier service is unavailable."
                        }
                      }
                    }
                  }
                }
              }
            }
          },
          "500": {
            "description": "internal server error",
